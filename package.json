{
  "scripts": {
    "ng": "ng",
    "serve": "node --max_old_space_size=4096  ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --build-optimizer=false --configuration=production && npm run theming",
    "build:devops": "npm run build && grunt copy:output --output=\"%npm_config_output%\"",
    "analyze": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "theming": "grunt copy:theme && node ./node_modules/less/bin/lessc --js --clean-css \"./dist/theme/styles/theme.less\" \"./dist/assets/theme.css\" && node ./compilation/rename-theme-image.js",
